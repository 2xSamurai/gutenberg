{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "core/calendar",
	"title": "Calendar",
	"category": "widgets",
	"description": "A calendar of your siteâ€™s posts.",
	"keywords": [ "posts", "archive" ],
	"textdomain": "default",
	"attributes": {
		"month": {
			"type": "integer"
		},
		"year": {
			"type": "integer"
		}
	},
	"supports": {
		"align": true,
