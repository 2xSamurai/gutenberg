{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "core/calendar",
	"title": "Calendar",
	"category": "widgets",
	"description": "A calendar of your siteâ€™s posts.",
	"keywords": [ "posts", "archive" ],
	"textdomain": "default",
	"attributes": {
		"month": {
			"type": "integer"
		},
		"year": {
			"type": "integer"
		}
	},
	"supports": {
		"align": true,
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"__experimentalSelector": "th, td",
			"__experimentalSkipSerialization": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"width": true
			}
		},
		"color": {
			"link": true,
			"background": true,
			"__experimentalSelector": "thead, tbody",
			"__experimentalDefaultControls": {
				"text": true
			}
		},
		"__experimentalStyle": {
			"border": {
				"style": "solid",
				"width": "1px"
			}
		}
	},
	"style": "wp-block-calendar"
}
